{
  "expo": {
    "name": "YakBak",
    "slug": "YakBak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yakbak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEE9D7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.grisu.yakbak",
      "appleTeamId": "RUCL895JKR"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FEE9D7"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.grisu.yakbak"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FEE9D7"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
